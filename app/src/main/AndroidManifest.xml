<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="io.ditclear.app">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".partial.PartialRefreshListKotlin" android:label="PartialRefreshList"/>
        <activity android:name=".multitype.MultiTypeListKotlin" android:label="MultiTypeList"/>
        <activity android:name=".singletype.SingleTypeListKotlin" android:label="SingleTypeList"/>
    </application>

</manifest>